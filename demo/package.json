{
  "name": "gulp.pug",
  "version": "1.0.0",
  "description": "Gulp, SASS + Pug Templates",
  "main": "gulpfile.js",
  "private": true,
  "scripts": {
    "start": "gulp",
    "vendor": "gulp vendor-scripts",
    "css": "gulp sass",
    "js": "gulp js",
    "html": "gulp templates",
    "update-all": "npm run css && npm run js && npm run html && echo 'success!'",
    "clear:dist": "rm -Rf ./dist/",
    "clear:packages": "rm -Rf ./bower_components ./node_modules",
    "webpack": "./node_modules/.bin/webpack",
    "webpack-help": "webpack --help",
    "webpack.dev": "webpack -d --watch",
    "webpack.build": "webpack -p"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.17.5",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-pug": "^3.1.0",
    "gulp-sass": "^2.3.2",
    "gulp-uglifyjs": "^0.6.2",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
}

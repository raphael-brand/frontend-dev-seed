{
  "name": "gulp.pug",
  "version": "1.0.0",
  "description": "Gulp, SASS + Pug Templates",
  "main": "Gulpfile.js",
  "private": true,
  "scripts": {
    "start": "npm run check-vendor && gulp",
    "check-vendor": "test -d ./node_modules && npm run vendor || (npm install && gulp vendor-scripts) || (echo 'npm not installed. Please run \"npm install -g npm\"' && exit 1)",
    "vendor": "gulp vendor-scripts",
    "css": "gulp sass",
    "js": "gulp js",
    "html": "gulp templates",
    "update-all": "npm install && gulp sass js templates && echo 'success!'",
    "clear:dist": "rm -Rf ./dist/",
    "clear:packages": "rm -Rf ./node_modules"
  },
  "author": "Raphael Brand<info@raphaelbrand.de>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "bootstrap-xxl": "^4.1.0",
    "browser-sync": "^2.24.4",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-cache": "^1.0.2",
    "gulp-minify-css": "^1.2.4",
    "gulp-clean-css": "^3.9.4",
    "gulp-concat": "^2.6.1",
    "gulp-eslint": "^4.0.2",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-minify-html": "^1.0.6",
    "gulp-plumber": "^1.2.0",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "jquery": "^1.9.1",
    "minimatch": "^3.0.4",
    "popper.js": "^1.14.3",
    "requirejs": "^2.3.5",
    "uuid": "^3.2.1"
  }
}

{
  "name": "exploring-typescript-and-three.js",
  "version": "1.0.0",
  "description": "Gulp, SASS + Pug Templates",
  "main": "Gulpfile.js",
  "private": true,
  "scripts": {
    "start": "npm run check-vendor && gulp",
    "check-vendor": "test -d node_modules || (npm install) || (echo 'npm not installed. Please run \"npm install -g npm\"' && exit 1)",
    "vendor": "gulp vendor-scripts",
    "css": "gulp sass",
    "js": "gulp js",
    "html": "gulp templates",
    "update-all": "npm install && gulp sass js templates && echo 'success!'",
    "clear:dist": "rm -Rf ./dist/",
    "clear:packages": "rm -Rf ./node_modules"
  },
  "author": "Raphael Brand<info@raphaelbrand.de>",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.17.5",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-mdcs": "^4.2.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-typescript": "^0.3.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-pug": "^3.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-typescript": "^3.2.1",
    "gulp-uglify": "^3.0.0",
    "nodemon": "^1.11.0",
    "requirejs": "^2.3.3",
    "typescript": "^2.4.2",
    "typescript-eslint-parser": "^4.0.0"
  },
  "dependencies": {
    "bootstrap-sass": "^3.3.7",
    "three": "^0.86.0"
  }
}

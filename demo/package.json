{
  "name": "gulp.pug",
  "version": "1.0.0",
  "description": "Gulp, SASS + Pug Templates",
  "main": "Gulpfile.js",
  "private": true,
  "scripts": {
    "start": "npm run check-vendor && gulp",
    "check-vendor": "test -d ./node_modules && npm run vendor || (npm install && gulp vendor-scripts) || (echo 'npm not installed. Please run \"npm install -g npm\"' && exit 1)",
    "vendor": "gulp vendor-scripts",
    "css": "gulp sass",
    "js": "gulp js",
    "html": "gulp templates",
    "update-all": "npm install && gulp sass js templates && echo 'success!'",
    "clear:dist": "rm -Rf ./dist/",
    "clear:packages": "rm -Rf ./node_modules"
  },
  "author": "Raphael Brand<info@raphaelbrand.de>",
  "license": "MIT",
  "devDependencies": {
    "bootstrap-xxl": "^4.1.0",
    "browser-sync": "^2.17.5",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-pug": "^3.1.0",
    "gulp-rename": "^1.2.1",
    "gulp-sass": "^2.0.0",
    "gulp-uglify": "^3.0.0",
    "del": "^1.1.1",
    "gulp-cache": "^0.2.8",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-eslint": "^0.13.2",
    "gulp-load-plugins": "^0.10.0",
    "gulp-util": "3.0.6",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.2.1",
    "gulp-minify-css": "^1.1.1",
    "gulp-minify-html": "^1.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-babel": "^5.2.1",
    "requirejs": "^2.3.3"
  }
}
